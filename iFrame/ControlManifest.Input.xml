<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <!-- Defines the control's main properties such as namespace, constructor, version, and display names -->
  <control namespace="iFrame" constructor="iFrame" version="0.0.3" display-name-key="iFrame" description-key="iFrame description" control-type="standard">
    <!-- Declares that the PCF control does not use an external service -->
    <external-service-usage enabled="false"></external-service-usage>
    
    <!-- Properties that the control expects from the Common Data Service (CDS), configuring URL and optional height and query string parameters -->
    <property name="UrlValue" display-name-key="UrlValue" description-key="UrlValue" of-type="SingleLine.URL" usage="bound" required="true" />
    <property name="Height" display-name-key="Height" description-key="Height" of-type="Whole.None" usage="input" />
    <property name="QueryStringName" display-name-key="QueryStringName" description-key="QueryStringName description" of-type="SingleLine.Text" usage="input" />
    <property name="QueryStringValue" display-name-key="QueryStringValue" description-key="QueryStringValue description" of-type="SingleLine.Text" usage="input" />
    
    <!-- Defines the resources used by the control, in this case, only a TypeScript file -->
    <resources>
      <code path="index.ts" order="1"/>
    </resources>
  </control>
</manifest>
